{
  "name": "applications-tracker",
  "version": "0.1.0",
  "private": true,
  "main": "build/electron/main.js",
  "scripts": {
    "dev": "concurrently \"yarn start\" \"yarn start:electron\"",
    "start": "cross-env BROWSER=none react-scripts start",
    "start:electron": "yarn run build:electron && cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "build": "react-scripts build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rebuild": "yarn run electron-rebuild"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^7.4.3",
    "better-sqlite3-helper": "^3.1.5",
    "effector": "^22.1.2",
    "effector-react": "^22.0.4",
    "electron-is-dev": "^2.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-use": "^17.3.1",
    "styled-components": "^5.3.1",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.4.0",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@types/styled-components": "^5.1.15",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "effector-logger": "^0.13.0",
    "electron": "^15.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "react-scripts": "4.0.3",
    "redux-devtools": "^3.7.0",
    "typescript": "^4.4.4"
  }
}
